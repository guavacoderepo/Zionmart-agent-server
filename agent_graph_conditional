// Agentic Customer Care Graph
digraph {
	size="12,10"
	rankdir=TB
	_start_ [label=_start_]
	rewriter [label=Rewriter]
	supervisor [label=Supervisor]
	order [label=Order]
	enquiry [label=Enquiry]
	complaint [label=Complaint]
	off_topic [label=Off_topic]
	create_order [label=Create_order]
	edit_order [label=Edit_order]
	cancel_order [label=Cancel_order]
	retrieve_items [label=Retrieve_items]
	_end_ [label=_end_]
	_start_ -> rewriter [color=black]
	rewriter -> supervisor [color=black]
	supervisor -> enquiry [color=green]
	supervisor -> complaint [color=red]
	supervisor -> off_topic [color=gray]
	supervisor -> order [color=orange]
	order -> create_order [color=blue]
	order -> edit_order [color=blue]
	order -> cancel_order [color=blue]
	order -> retrieve_items [color=blue]
	create_order -> supervisor [color=black]
	edit_order -> supervisor [color=black]
	enquiry -> supervisor [color=black]
	complaint -> supervisor [color=black]
	cancel_order -> supervisor [color=black]
	retrieve_items -> supervisor [color=black]
	supervisor -> _end_ [color=black]
	off_topic -> _end_ [color=black]
}
